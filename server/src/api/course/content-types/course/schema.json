{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "courses"
    },
    "Course_name": {
      "type": "string"
    },
    "course_img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "rating": {
      "type": "float",
      "max": 5,
      "min": 0
    },
    "course_chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-chapter.course-chapter",
      "mappedBy": "course"
    },
    "price": {
      "type": "decimal"
    },
    "course_syllabus": {
      "type": "blocks"
    },
    "course_hour": {
      "required": false,
      "type": "integer"
    },
    "course_minute": {
      "type": "integer"
    },
    "course_description": {
      "type": "richtext"
    },
    "course_overview": {
      "type": "richtext"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "courses"
    }
  }
}
