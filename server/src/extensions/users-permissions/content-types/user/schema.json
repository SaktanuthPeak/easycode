{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "UID": {
      "type": "integer",
      "unique": true
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "users"
    },
    "user_group": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    },
    "instructor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::instructor.instructor",
      "mappedBy": "instructor"
    }
  }
}
